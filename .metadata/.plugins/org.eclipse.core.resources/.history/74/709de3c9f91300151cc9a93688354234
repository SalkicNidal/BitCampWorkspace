package ba.bitcamp.lectures.task1;

public class User {

	private String name;
	private String surname;
	private Integer age;
	
	
	public User(String name, String surname) {
		super();
		this.name = name;
		this.surname = surname;
	}


	public User(String name, String surname, Integer age) {
		super();
		this.name = name;
		this.surname = surname;
		this.age = age;
	}
	
	
	
	
	
}
