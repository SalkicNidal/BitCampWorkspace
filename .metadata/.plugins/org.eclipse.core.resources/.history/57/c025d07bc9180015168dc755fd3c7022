	public static  Compare  getMax(Compare[] f) {
		Compare max = f[0];
		for (int i = 0; i < f.length; i++) {
			if (f[i].comapre(max) == 1) {
				max = f[i];
			}

		}
		return max;
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Foo[] arr = new Foo[5];
		arr[0] = new Foo(3);
		arr[1] = new Foo(1);
		arr[2] = new Foo(8);
		arr[3] = new Foo(3);
		arr[4] = new Foo(4);

		Compare n = Main.getMax(arr);

		System.out.println(n);
		
		
		Bar [] barArray = new Bar[3];
		barArray[0] = new Bar("Nidal");
		barArray[1] = new Bar ("AB");
		barArray[2] = new Bar ("AAA");
		
		System.out.println(Main.getMax(barArray).toString());
		
		OuterClass oc = new OuterClass();
		oc.ic.PrintVariables(number);

	}

}
